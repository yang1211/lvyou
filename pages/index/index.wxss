page{overflow-x: hidden}
.search
{
  display: flex;
  flex-direction: row;
  justify-content:space-between;
  align-items: center;
  width:85%;
  height:60rpx;
  background-color:whitesmoke;
  margin:1% auto;
  border:1rpx solid whitesmoke;
  border-radius: 5rpx;
  position:fixed;
  top:0;
  left:0;
  right:0;
  z-index:88;
}

/* 搜索框提示文字样式 */
.search input
{
  font-size:27rpx;
  padding-left:30rpx; 
}

/* 搜索框小图标 */
.search image
{ 
  width:40rpx;
  height:40rpx;
  padding-right:20rpx;
}
swiper{
  width: 100%;
  height: 400rpx;
  margin-top:90rpx;

}
swiper image{
  display: block;
  height: 100%;
  width: 100%;
}
.swiperView {
  height: 350rpx;
  background: #ddd;
  width: 100%;
}

.function {
  display: flex;
  width: 100%;
}
.function_item {
  margin: 20rpx;
  margin-bottom: 10rpx;
  flex: 1;
  width: 211rpx;
  border-radius: 12rpx;
  position: relative;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
}
.width1{
width: 25%;
}
.width2{
width: 50%;
}
.width3{
width: 75%;
}
.width4{
width: 100%;
}
.title {
  display: flex;
  justify-content: left;
  line-height: 28rpx;
  margin-top: 20rpx;
  font-size: 14px;
}
.image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.rexiao{
